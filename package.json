{
  "name": "link-preview-js",
  "version": "2.1.6",
  "description": "Javascript module to extract and fetch HTTP link information from blocks of text. ",
  "types": "source/globals.d.ts",
  "scripts": {
    "watch": "tsc -w",
    "dev": "nodemon source/app.ts",
    "test": "jest",
    "test:ci": "jest --testLocationInResults --ci --outputFile=test_results.json --json",
    "build": "tsc",
    "publishPackage": "tsc && jest --detectOpenHandles && npm publish "
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "cors": "^2.8.5",
    "cross-fetch": "3.0.6",
    "dompurify": "^2.2.9",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "interweave": "^12.7.2",
    "jsonfile": "^6.1.0",
    "url": "0.11.0"
  },
  "files": [
    "build"
  ],
  "devDependencies": {
    "@skypack/package-check": "^0.2.2",
    "@types/cheerio": "0.22.24",
    "@types/cors": "^2.8.10",
    "@types/dompurify": "^2.2.2",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/jsonfile": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "2.2.1",
    "ts-jest": "^26.5.6",
    "typescript": "^4.3.4"
  },
  "private": true
}
